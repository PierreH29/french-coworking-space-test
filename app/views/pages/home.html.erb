<div class='wrapper' style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('https://unsplash.com/photos/Pd8tLVGx2O4'); background-size: cover">
  <%= render 'shared/navbar' %>

  <div class="centered landing-page">
    <h1>French Coworking Space</h1>
    <p class="lead">Welcome to your cool new coworking space</p>
    <% unless current_user %>
      <%= link_to 'Login', login_path, class: 'btn btn-info' %>
    <% else %>
      <%= link_to 'Go to dashboard', dashboard_path, class: 'btn btn-info' %>
    <% end %>
  </div>

</div>
